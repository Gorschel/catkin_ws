<?xml version="1.0"?>
<launch>

    <!--launch robot nodes-->
    <include file="$(find control_dual_robot)/launch/robot_0.launch"/>
    <include file="$(find control_dual_robot)/launch/robot_1.launch"/>

    <!--launch two phase solver services-->
    <include file="$(find control_dual_robot)/launch/solver.launch"/>

    <!--launch control node-->
    <node name="control" pkg="control_dual_robot" type="control.py" output="screen"/>

</launch>